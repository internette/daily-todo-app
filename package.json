{
  "name": "daily-todo",
  "productName": "DailyToDo",
  "version": "1.0.0",
  "description": "An unobtrusive desktop app to keep track of daily tasks that resets at midnight",
  "main": "main.js",
  "scripts": {
    "watch": "webpack --watch --progress --debug",
    "build": "webpack --progress",
    "app": "electron .",
    "app:osx-test": "export ELECTRON_ENABLE_LOGGING=true && electron .",
    "app:windows-test": "set ELECTRON_ENABLE_LOGGING=true && electron .",
    "pack:osx": "electron-packager . $npm_package_productName --out=dist/osx --platform=darwin --arch=x64 --icon=src/icons/32x32.icns --overwrite",
    "pack:win32": "electron-packager . $npm_package_productName --out=dist/win --platform=win32 --arch=ia32 --overwrite",
    "pack:win64": "electron-packager . $npm_package_productName --out=dist/win --platform=win32 --arch=x64 --icon=src/icons/32x32.ico --overwrite",
    "pack:all": "npm run pack:osx && npm run pack:win32 && npm run pack:win64"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/twaffl3s/daily-todo.git"
  },
  "author": "Antoinette Janus",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/twaffl3s/daily-todo/issues"
  },
  "homepage": "https://github.com/twaffl3s/daily-todo#readme",
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "css-loader": "^0.28.0",
    "electron": "^1.6.6",
    "node-sass": "^4.5.2",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.16.1"
  },
  "dependencies": {
    "babel-cli": "^6.24.1",
    "electron-config": "^0.2.1",
    "moment": "^2.18.1",
    "prop-types": "^15.5.9",
    "react": "^15.5.4",
    "react-custom-scrollbars": "^4.1.1",
    "react-dom": "^15.5.4",
    "react-redux": "^5.0.4",
    "redux": "^3.6.0",
    "redux-electron-ipc": "^1.1.9",
    "webpack": "^2.4.1"
  }
}
